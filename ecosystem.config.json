{
  "apps": [
    {
      "name": "em2024-frontend",
      "script": "node ./dist/server/entry.mjs",
      "port": 4322,
      "watch": false,
      "autorestart": true,
      "restart_delay": 5000,
      "log_date_format": "YYYY-MM-DD HH:mm:ss",
